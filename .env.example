# Telegram API Credentials
# Get these from https://my.telegram.org/apps
API_ID=your_api_id
API_HASH=your_api_hash

# Session Name
# This will be the name of the session file
SESSION_NAME=telegram_session_name

# Telegram Group IDs
# ACLEDA group ID (get from group info)
GROUP_ID=your_acleda_group_id
# PayWay group ID (get from group info)
GROUP_ID_PAYWAY=your_payway_group_id

# Bot Token
# Get from @BotFather on Telegram
BOT_TOKEN=your_bot_token

# Admin User ID
# Your Telegram user ID (get from @userinfobot)
ADMIN_USER_ID=your_telegram_user_id
